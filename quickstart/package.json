{
  "name": "get-started",
  "version": "0.0.1",
  "description": "Restack Get started example",
  "scripts": {
    "dev": "tsx watch --include src src/services.ts",
    "build": "tsc --build",
    "schedule-workflow": "tsx scheduleWorkflow.ts",
    "restack-up": "tsx restackUp.ts",
    "restack-web-ui": "docker run -d --pull always --name restack -p 5233:5233 -p 6233:6233 -p 7233:7233 ghcr.io/restackio/restack:main",
    "restack:up": "dotenv -e .env node restack_up.mjs",
    "docker:build": "docker build -t restack-get-started .",
    "docker:run": "docker run -d --name restack-get-started -p 3000:3000 restack-get-started"
  },
  "dependencies": {
    "@restackio/ai": "^0.0.93",
    "@temporalio/workflow": "1.11.3"
  },
  "devDependencies": {
    "@types/node": "20.16.9",
    "dotenv-cli": "^7.4.2",
    "prettier": "3.3.3",
    "tsx": "4.19.2",
    "typescript": "5.6.3"
  }
}
